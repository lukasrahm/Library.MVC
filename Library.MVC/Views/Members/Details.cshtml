@model Library.Domain.Member

@{
    ViewData["Title"] = "Details";
}

<div>
    <h4>Bokdetaljer</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            ISBN
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.SSN)
        </dd>
        <dt class="col-sm-2">
            Titel
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Redigera</a> |
    <a asp-action="Index">Tillbaka</a>
</div>